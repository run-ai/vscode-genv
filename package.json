{
  "name": "vscode-genv",
  "displayName": "vscode-genv",
  "description": "Visual Studio Code extension for genv",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vscode-genv.activate",
    "onCommand:vscode-genv.config.gpus",
    "onCommand:vscode-genv.config.name",
    "onCommand:vscode-genv.attach",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "vscode-genv.activate",
        "title": "GPU: Activate Environment"
      },
      {
        "command": "vscode-genv.config.gpus",
        "title": "GPU: Configure Environment Device Count"
      },
      {
        "command": "vscode-genv.config.name",
        "title": "GPU: Configure Environment Name"
      },
      {
        "command": "vscode-genv.attach",
        "title": "GPU: Attach Devices"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.70.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {}
}
